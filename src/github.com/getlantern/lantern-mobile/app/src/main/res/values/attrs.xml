<resources>
    <declare-styleable name="TitleBar">
        <attr name="backgroundColor" format="integer" />
        <attr name="titleText" format="string" />
        <attr name="textColor" format="integer" />
        <attr name="titleImage" format="integer" />
        <attr name="disableBackButton" format="boolean" />
    </declare-styleable>

    <declare-styleable name="UserForm">
        <attr name="sendBtnText" format="string" />
        <attr name="formIcon" format="reference" />
        <attr name="formType" format="string" />
        <attr name="formList" format="reference" /> 
        <attr name="inputType" format="string" />
    </declare-styleable>
</resources>
